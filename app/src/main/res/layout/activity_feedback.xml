<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f7f7f7"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.gdg.projectniger.Feedback">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:gravity="center"
    android:id="@+id/linearLayout2">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:paddingBottom="15dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:id="@+id/linearLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/linearLayout3"
        android:layout_alignEnd="@+id/linearLayout3">
        <ImageView
            android:onClick="calendar"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp" />
        <ImageView
            android:onClick="maps"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_marginLeft="0dp" />
        <ImageView
            android:onClick="youtube"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_marginLeft="0dp"/>
        <ImageView
            android:onClick="uiux"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_marginLeft="0dp" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:paddingBottom="15dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_above="@+id/linearLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/linearLayout2"
        android:layout_alignEnd="@+id/linearLayout2"
        android:id="@+id/linearLayout3">
        <ImageView
            android:onClick="androidstudio"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp" />
        <ImageView
            android:onClick="android"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_marginLeft="0dp"/>
        <ImageView
            android:onClick="playstore"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_marginLeft="0dp" />
        <ImageView
            android:onClick="chrome"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_marginLeft="0dp" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="message"
        android:id="@+id/textView11"
        android:layout_below="@+id/linearLayout2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="46dp" />


</RelativeLayout>
